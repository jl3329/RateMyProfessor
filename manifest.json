{
  "manifest_version": 2,

  "name": "Rate My Professor",
  "description": "This extension shows information from Ratemyprofessor.com based on what the user highlights.",
  "version": "0.1",
  
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["eventpage.js"],
    "persistent": false
  },
  "permissions": [
    "tabs",
    "activeTab",
    "http://www.ratemyprofessors.com/*"
  ]
}
